function stream_data_menu_links_discovered_alter(&$links) {
  // Check if the menu link is your desired link.
  if (isset($links['streams'])) {
    // If the current user is anonymous, hide the menu link.
    if (\Drupal::currentUser()->isAnonymous()) {
      $links['streams']['hidden'] = TRUE;
    }
  }
}
